class Solution {
    public int countPrimeSetBits(int left, int right) {
        int ans=0;
        for(int i=left;i<=right;i++) {
            int x=i;
            int c=0;
            while(x!=0) {
                if((x&1)!=0) c++;
                x>>=1;
            }
            if(c==2 || c==3 || c==5 || c==7 || c==11 || c==13 || c==17 || c==19 || c==13 || c==19 || c==23 || c==29 || c==31) ans++;
        }
        return ans;
    }
}